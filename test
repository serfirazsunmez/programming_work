print(result)

x = "Serfiraz"
y = 12
z = 4.2
print(type(x))
print(type(y))
print(type(z))
result = y + z
print(result)
print(type(result))
result = y + int(z)
print(result)
print(type(result))
result = z + float(y)
print(result)
print(type(result))
result = z
print(result)
print(type(str(z)))
print()
result = x + str(y) + str(z)
print(result)
print(type(result))
print()


