my_text = "Python Java C++"
my_text = my_text.split()
print(my_text)

my_text = "Hello, World"
my_text = my_text.upper()
print(my_text)

my_text = "I have an apple, you have an apple"
orange = "orange"
my_text = my_text.replace("apple", orange)
print(my_text)

my_text = "    I have an apple, you have an apple.    "
my_text = my_text.strip() # removes the spaces in the text above
print(my_text)


my_text = "Bananas are a great source of Potassium"
my_text = my_text.count("a")
print(my_text)

my_text = "Bananas are great source of Potassium"
my_text = len(my_text)
print(my_text)

my_first_text = "It is "
